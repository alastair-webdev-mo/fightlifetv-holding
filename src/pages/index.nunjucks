{% block left %}
<div class="content left">
	<div class="logo">
		<img src="dist/img/logo.png" width="175" class="ret">
		<nav class="site-links">
			<ul>
				<li data-tab="schedule"><a href="">Schedule</a></li>
				<li data-tab="videos"><a href="">Videos</a></li>
				<li data-tab="about"><a href="">About</a></li>
			</ul>
		</nav>
	</div>
	<div class="social-media">
		<ul>
			<li><a href="https://www.facebook.com/fightlifetvuk/?fref=ts" target="_blank">Facebook</a></li>
			<li><a href="https://twitter.com/RealFightLifeTV" target="_blank">Twitter</li>
			<li class="last"><a href="https://www.youtube.com/channel/UCyov0S9VF_houc4IoTB7hCQ" target="_blank">Youtube</a></li>
		</ul>
